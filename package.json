{
  "name": "task-manager-demo",
  "version": "1.0.0",
  "description": "A simple RESTful API for managing tasks, built with Express.js",
  "main": "src/server.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "json-summary"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PankajTanwar7/task-manager-demo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PankajTanwar7/task-manager-demo/issues"
  },
  "homepage": "https://github.com/PankajTanwar7/task-manager-demo#readme",
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "winston": "^3.18.3",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4"
  }
}
